#%Module1.0
# Python 2.7 module file
#

proc ModulesHelp { } {
global version modroot

puts stderr "Configura o ambiente para a utilização do Python 2.7"
}

module-whatis "Configura o ambiente para a utilização do Python 2.7"

set topdir /opt/python2.7
set version 2.7.11

prepend-path PATH $topdir/bin
prepend-path LD_LIBRARY_PATH $topdir/lib