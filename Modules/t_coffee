#%Module1.0
# tcoffee module file
#

proc ModulesHelp { } {
global version modroot

puts stderr "Configura o ambiente para a utilização do tcoffee"
}

module-whatis "Configura o ambiente para a utilização do tcoffee"

set topdir /opt/bio/tcoffee
set version 1.0
set sys linux64

prepend-path PATH $topdir/bin