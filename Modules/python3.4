#%Module1.0
# Python 3.4 module file
#

proc ModulesHelp { } {
global version modroot

puts stderr "Configura o ambiente para a utilização do Python 3.4"
}

module-whatis "Configura o ambiente para a utilização do Python 3.4"

set topdir /opt/python3.4
set version 3.4.

prepend-path PATH $topdir/bin
prepend-path LD_LIBRARY_PATH $topdir/lib