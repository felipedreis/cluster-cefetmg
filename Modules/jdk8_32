#%Module1.0
# Java 8_32 module file
#
proc ModulesHelp { } {
        global version modroot
        puts stderr "Configura o ambiente para a utilização do JDK 8 32 bits"
}
module-whatis   "Configura o ambiente para a utilização do JDK 8 32 bits"
set     topdir          /opt/java/jdk1.8.0_92_32bits
set     version         1.8.0_92
set     sys             linux32
setenv		JAVA_HOME	$topdir
prepend-path    PATH            $topdir/bin
