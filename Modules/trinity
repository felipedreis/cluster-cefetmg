#%Module1.0
# Trinity 2.2.0 module file
#

proc ModulesHelp { } {
global version modroot

puts stderr "Configura o ambiente para a utilização do Trinity 2.2.0"
}

module-whatis "Configura o ambiente para a utilização do Trinity 2.2.0"

set topdir /opt/trinity-2.2.0
set version 2.2.0

prepend-path PATH $topdir/bin
prepend-path LD_LIBRARY_PATH $topdir/lib